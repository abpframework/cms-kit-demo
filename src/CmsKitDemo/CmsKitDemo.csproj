  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Volo.Abp.Studio.Client.AspNetCore" Version="1.2.2" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Volo.Abp.AspNetCore.Mvc" Version="9.3.3" />
    <PackageReference Include="Volo.Abp.Autofac" Version="9.3.3" />
    <PackageReference Include="Volo.Abp.AutoMapper" Version="9.3.3" />
    <PackageReference Include="Volo.Abp.Swashbuckle" Version="9.3.3" />
    <PackageReference Include="Volo.Abp.AspNetCore.Serilog" Version="9.3.3" />
    <PackageReference Include="Volo.Abp.EntityFrameworkCore.Sqlite" Version="9.3.3" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Volo.Abp.Account.Application" Version="9.3.3" />
