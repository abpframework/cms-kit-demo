  <ItemGroup>
    <PackageReference Include="Volo.Abp.PermissionManagement.Domain.Identity" Version="9.3.3" />
    <PackageReference Include="Volo.Abp.Identity.Application" Version="9.3.3" />
    <PackageReference Include="Volo.Abp.Identity.HttpApi" Version="9.3.3" />
    <PackageReference Include="Volo.Abp.Identity.EntityFrameworkCore" Version="9.3.3" />
    <PackageReference Include="Volo.Abp.OpenIddict.EntityFrameworkCore" Version="9.3.3" />
